{
  "name": "contentful-inventory",
  "version": "2.0.0",
  "description": "Comprehensive tool for migrating content from Contentful to WordPress with table integration support",
  "main": "index.js",
  "scripts": {
    "inventory": "tsx src/inventory.ts",
    "typecheck": "tsc --noEmit",
    "analyze": "node src/migration/analyze-tables.js",
    "extract-tables": "node src/contentful/extract-tables.js",
    "migrate": "node src/migration/migrate-content.js",
    "install-plugin": "node src/wordpress/install-plugin.js",
    "test-connection": "node src/utils/test-connection.js",
    "restore-content": "node src/migration/restore-content.js",
    "import-guides": "node src/migration/import-guides.js",
    "extract-guide-tables": "node src/migration/extract-guide-tables.js",
    "analyze-guides": "node src/contentful/analyze-guides.js",
    "analyze-communities": "node src/contentful/analyze-community-pages.js",
    "import-states": "node src/migration/import-states.js",
    "import-cities": "node src/migration/import-cities.js",
    "extract-charts": "node src/migration/extract-charts.js",
    "sync-to-prod": "node src/migration/sync-to-prod.js",
    "sync-to-prod:dry": "node src/migration/sync-to-prod.js --dry-run",
    "sync-to-prod:update": "node src/migration/sync-to-prod.js --update",
    "sync-to-prod:update:dry": "node src/migration/sync-to-prod.js --update --dry-run",
    "sync-media-to-prod": "node src/migration/sync-media-to-prod.js",
    "sync-communities": "node src/migration/sync-communities-to-prod.js",
    "sync-communities:update": "node src/migration/sync-communities-to-prod.js --update",
    "sync-communities:update:dry": "node src/migration/sync-communities-to-prod.js --update --dry-run",
    "sync-communities:dry": "node src/migration/sync-communities-to-prod.js --dry-run",
    "sync-communities:parents": "node src/migration/sync-communities-to-prod.js --only=parents",
    "sync-communities:children": "node src/migration/sync-communities-to-prod.js --only=children",
    "save-provider-listings": "node src/migration/save-provider-listings.js",
    "save-provider-listings:dry": "node src/migration/save-provider-listings.js --dry-run",
    "remove-cards-shortcode": "node src/migration/remove-cards-shortcode.js",
    "remove-cards-shortcode:dry": "node src/migration/remove-cards-shortcode.js --dry-run",
    "remove-nav-shortcode": "node src/migration/remove-navigation-shortcode.js",
    "remove-nav-shortcode:dry": "node src/migration/remove-navigation-shortcode.js --dry-run",
    "migrate-images": "node src/migration/migrate-contentful-images.js",
    "migrate-images:dry": "node src/migration/migrate-contentful-images.js --dry-run",
    "migrate-images:report": "node src/migration/migrate-contentful-images.js --report",
    "build": "tsc",
    "dev": "tsx --watch src/inventory.ts",
    "clean": "rm -rf out/* temp/*"
  },
  "keywords": [
    "contentful",
    "wordpress",
    "migration",
    "cms",
    "headless",
    "tables",
    "content-migration",
    "api-integration"
  ],
  "author": "sanruiz",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sanruiz/contentful-inventory.git"
  },
  "type": "module",
  "dependencies": {
    "contentful-management": "^11.68.0",
    "dotenv": "^17.2.3",
    "mysql2": "^3.16.3",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@types/node": "^25.0.10",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}